{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es6",
    "module": "commonjs",
    "outDir": "out",
    "types": [
      "jasmine",
      "node"
    ],
    "moduleResolution": "node",
    "sourceMap": true
  },
    "files": [
    "test.ts",
    "polyfills.ts"
  ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts",
    "src/**/*.ts"
  ],
  "exclude": [
      "out",
    "node_modules"
  ]
}
